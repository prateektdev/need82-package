{
  "parser": "babel-eslint",
   "parserOptions": {
        "ecmaVersion": 2017
    },
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "jquery": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "react/display-name": 1,
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-extra-boolean-cast": 0,
    "eqeqeq": 2,
    "max-len": [
      0,
      80,
      4,
      {
        "ignoreUrls": true
      }
    ]
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}