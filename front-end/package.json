{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^16.0.0",
    "@ckeditor/ckeditor5-react": "^2.0.0",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.11",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^8.2.0",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "aws-amplify": "^1.2.4",
    "aws-amplify-react": "^2.5.4",
    "bnc-assist": "^0.10.3",
    "bootstrap": "^4.3.1",
    "draft-js": "^0.11.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "ethereumjs-tx": "^2.1.1",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "lato-webfont": "^2.15.1",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-accessible-accordion": "^3.0.1",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-collapsible": "^2.6.2",
    "react-dom": "^16.12.0",
    "react-draft-wysiwyg": "^1.14.1",
    "react-infinite-scroll-component": "^5.0.4",
    "react-moment": "^0.9.7",
    "react-multi-carousel": "^2.4.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-star-rating-component": "^1.4.1",
    "react-step-progress-bar": "^1.0.3",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "typescript": "3.7.2",
    "web3": "^1.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts --max_old_space_size=8192 build && npm run lint && npm run lint",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "rm -rf start && npm run build && node server.js",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "airbnb",
      "plugin:react/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "no-console": "off",
      "react/prop-types": "off",
      "react/display-name": 1,
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-extra-boolean-cast": 0,
      "eqeqeq": 2,
      "max-len": [
        0,
        80,
        4,
        {
          "ignoreUrls": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0"
  }
}
